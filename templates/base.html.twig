<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Nom du site
			{% endblock %}
		</title>
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
		<link
		rel="icon" type="image/png" href="data:image/png;base64,">
	{#{% block javascripts %}
						            {{ encore_entry_script_tags('app') }}
						
						    {% endblock %}#}
	</head>
	<body>
		<div class="grid grid-cols-3">
			<header class="flex justify-end">
				{% if is_granted('ROLE_USER') %}
					{% include 'layouts/header.html.twig' %}
				{% endif %}
			</header>
			<main class="w-full ml-0 mr-0 {% if is_granted('ROLE_USER') %}{% endif %} "> {% block body %}{% endblock %}
				</main>
				<div class="flex justify-start">
					{% if is_granted('ROLE_USER') %}
						{% include 'layouts/footer.html.twig' %}
					{% endif %}
				</div>
			</div>
		</body>
	</html>
